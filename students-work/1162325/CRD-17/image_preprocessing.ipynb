    "def gaussian(img,gaussian_kernel=5):\n",
    "    \n",
    "    if(len(img.shape)>2):\n",
    "        img = cv2.GaussianBlur(img,(gaussian_kernel,gaussian_kernel),0)\n",
    "        img = cv2.cvtColor(img, cv2.COLOR_BGR2GRAY)\n",
    "        mid = 0.5\n",
